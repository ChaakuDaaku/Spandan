{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch index.html",
            "type": "edge",
            "request": "launch",
            "file": "${workspaceRoot}/index.html"
        },
       {
            "name": "Launch localhost with sourcemaps",
            "type": "edge",
            "request": "launch",
            "url": "http://localhost/index.html",
            "sourceMaps": true,
            "webRoot": "wwwroot"
        },
        {
            "name": "Attach",
            "type": "edge",
            "request": "attach",
            "port": 9222
        }
    ]
}